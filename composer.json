{
    "name": "sajadtorkamani/quote-cli",
    "description": "A CLI that prints inspiring quotes from my Notion database.",
    "keywords": [
        "quotes",
        "cli"
    ],
    "homepage": "https://github.com/sajadtorkamani/quote-cli",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sajadtorkamani/quote-cli/issues",
        "source": "https://github.com/sajadtorkamani/quote-cli/source",
    },
    "authors": [
        {
            "name": "Sajad Torkamani",
            "email": "sajadtorkamani1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^8.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["quote-cli"]
}
